    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "type": "module",
  "dependencies": {
    "table": "^6.8.2"
  }
}
