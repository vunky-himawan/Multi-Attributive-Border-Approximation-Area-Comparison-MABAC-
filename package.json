  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@classes/(.*)$": "<rootDir>/src/classes/$1",
      "^@interfaces/(.*)$": [
        "<rootDir>/src/interfaces/$1"
      ],
      "^@enum/(.*)$": [
        "<rootDir>/src/enum/$1"
      ],
      "^@services/(.*)$": [
        "<rootDir>/src/services/$1"
      ],
      "^@src/(.*)$": [
        "<rootDir>/src/*"
      ]
    }
  },
